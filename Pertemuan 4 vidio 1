public class MethodParameter1 {
    static void methodKu(String namaDepan, String namaBelakang) { //nama adl parameter
       String nama = namaDepan + " " + namaBelakang;
        System.out.println("Selamat datang " + nama);
    }
    public static void main(String[] args){
        //pemanggilan method dg argument
        methodKu("Ichsan","Fathur Rahman"); // Ichsan adl argument
    }
}
